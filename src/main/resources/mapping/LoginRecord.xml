<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kuncms.login.dao.LoginRecordDao" >
	
    
    <insert id="insertNewLogrec"  parameterType="com.kuncms.login.model.LoginRecord">
		insert into login_record(id,user_name,user_id,login_time) 
		   values(#{id},#{user_name},#{user_id},#{login_time})
 	</insert>
  

</mapper>