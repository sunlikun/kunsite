<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kuncms.duihuan.dao.DuihuanRecordDao" >

  <select id="query"  parameterType="com.kuncms.duihuan.model.DuihuanRecord"  resultType="com.kuncms.duihuan.model.DuihuanRecord">
        SELECT*FROM duihuan_record l
        <where>
			<if test="user_id != null and user_id != ''">
				and l.user_id=#{user_id}
			</if>
			<if test="duihuan_code != null and duihuan_code != ''">
				and l.duihuan_code=#{duihuan_code}
			</if>
		</where>
		
     </select>
 	
 
 	<insert id="save"  parameterType="com.kuncms.duihuan.model.DuihuanRecord">

		insert into duihuan_record(id,user_id,creat_time,duihuan_code) 
		   values(#{id},#{user_id},#{creat_time},#{duihuan_code})
	</insert>
 
 	
</mapper>